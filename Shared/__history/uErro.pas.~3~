unit uErro;

interface

uses
  uInterfacesAPI;

type

  TErro = class;

  TErro = class(TInterfacedObject, iErro)
  private
    class var FErro: integer;
    class var FSucesso: integer;
  public
    class property Sucesso: integer read FSucesso write FSucesso;
    class property Erro: integer read FErro write FErro;
  end;

implementation

end.
